{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue", "html"
    ],
    "rules": {
        "indent": ["error", 4]
    },
    "settings": {
        "import/resolver": {
            "alias": {
                "map": [
                    ["@", "./src/"]
                ],
                "extensions": [".vue"]
            }
        }


    }
}
